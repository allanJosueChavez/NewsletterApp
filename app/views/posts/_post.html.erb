<div id="<%= dom_id post %>">
  <p>
    <strong>Title:</strong>
    <%= post.title %>
  </p>
  <p>
    <strong>Description:</strong>
    <%= post.description %>
  </p>
  <p>
    <strong>AUTHOR:</strong>
    <%= post.users_id %>
    <% username = User.find_by(id:post.users_id).email %>
    <%= link_to "#{username}", profile_path(:user_id => post.users_id) %>
  </p>
</div>
